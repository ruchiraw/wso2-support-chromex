<div class="threads">
    {{#.}}
        <div class="row thread">
            <div class="col-xs-12">
                <div class="row details">
                    <div class="col-xs-9 subject">
                        <a href="#" data-url="{{link}}" data-id="{{question_id}}">{{title}}({{answer_count}})</a>
                    </div>
                    <div class="col-xs-3 time">
                        <div class="pull-right">{{ago last_activity_date}}</div>
                    </div>
                </div>
            </div>
        </div>
    {{/.}}
</div>