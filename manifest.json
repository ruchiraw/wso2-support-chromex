{
    "manifest_version": 2,
    "name": "WSO2 Support",
    "description": "This extension gives you a few important points about the support issue being viewed",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "page.html"
    },
    "sandbox": {
        "pages": ["sandbox.html"]
    },
    "permissions": [
        "https://support.wso2.com/",
        "https://script.googleusercontent.com/",
        "https://script.google.com/",
        "https://www.google.com/",
        "https://fonts.googleapis.com/",
        "https://accounts.google.com/",
        "tabs",
        "notifications"
    ]
}
